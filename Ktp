
package ktp;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.Scanner;

public class Ktp {

    public static void main(String[] args) {
        int x, y;
        String n, a, jk, pk, gd, tl, tgl, k;
        Scanner sc = new Scanner(System.in);
        Scanner scn = new Scanner(System.in);
        Biodata st = new Biodata();
        
        HashMap<String, ArrayList> dataHash = new HashMap<String, ArrayList>();
        ArrayList<String> dataList = new ArrayList<String>();

        System.out.println("1.Kelurahan makmur");
        System.out.println("2.Kelurahan Jaya");
        System.out.println("Pilih kelurahan : ");
        x = sc.nextInt();
        switch (x) {
            case 1:
                st.setKelurahan("Makmur");
                System.out.println("pilih RT");
                System.out.println("1.RT 01");
                System.out.println("2.RT 02");
                System.out.println("3.RT 03");
                System.out.println("4.RT 04");
                System.out.println("5.RT 05");
                System.out.println("Input RT : ");
                y = sc.nextInt();

                switch (y) {
                    case 1:
                        st.setRT("01");
                        System.out.println("Input nama :");
                        n = scn.nextLine();
                        st.setNama(n);
                        System.out.println("Input agama :");
                        a = scn.nextLine();
                        st.setAgama(a);
                        System.out.println("Input jenis kelamin :");
                        jk = scn.nextLine();
                        st.setJenisKelamin(jk);
                        System.out.println("Input pekerjaan :");
                        pk = scn.nextLine();
                        st.setPekerjaan(pk);
                        System.out.println("Input golongan darah :");
                        gd = scn.nextLine();
                        st.setGolonganDarah(gd);
                        System.out.println("Input tempat lahir :");
                        tl = scn.nextLine();
                        st.setTempatLahir(tl);
                        System.out.println("Input tanggal lahir :");
                        tgl = scn.nextLine();
                        st.setTanggalLahir(tgl);
                        System.out.println("Input kewarganegaraan :");
                        k = scn.nextLine();
                        st.setKewarganegaraan(k);
                        dataList.add("" + st.getNama() + " " + st.getAgama() + " " + st.getJenisKelamin() + " " + st.getPekerjaan() + " " + st.getGolonganDarah() + " " + st.getKelurahan() + " " + st.getRT() + " " + st.getTempatLahir() + " " + st.getTanggalLahir() + " " + st.getKewarganegaraan() + ""); 
                        dataHash.put(st.getNama(), dataList);
                        break;
                    case 2:
                        st.setRT("02");
                        System.out.println("Input nama :");
                        n = scn.nextLine();
                        st.setNama(n);
                        System.out.println("Input agama :");
                        a = scn.nextLine();
                        st.setAgama(a);
                        System.out.println("Input jenis kelamin :");
                        jk = scn.nextLine();
                        st.setJenisKelamin(jk);
                        System.out.println("Input pekerjaan :");
                        pk = scn.nextLine();
                        st.setPekerjaan(pk);
                        System.out.println("Input golongan darah :");
                        gd = scn.nextLine();
                        st.setGolonganDarah(gd);
                        System.out.println("Input tempat lahir :");
                        tl = scn.nextLine();
                        st.setTempatLahir(tl);
                        System.out.println("Input tanggal lahir :");
                        tgl = scn.nextLine();
                        st.setTanggalLahir(tgl);
                        System.out.println("Input kewarganegaraan :");
                        k = scn.nextLine();
                        st.setKewarganegaraan(k);
                        dataList.add("" + st.getNama() + " " + st.getAgama() + " " + st.getJenisKelamin() + " " + st.getPekerjaan() + " " + st.getGolonganDarah() + " " + st.getKelurahan() + " " + st.getRT() + " " + st.getTempatLahir() + " " + st.getTanggalLahir() + " " + st.getKewarganegaraan() + ""); 
                        dataHash.put(st.getNama(), dataList);
                        break;
                    case 3:
                        st.setRT("03");
                        System.out.println("Input nama :");
                        n = scn.nextLine();
                        st.setNama(n);
                        System.out.println("Input agama :");
                        a = scn.nextLine();
                        st.setAgama(a);
                        System.out.println("Input jenis kelamin :");
                        jk = scn.nextLine();
                        st.setJenisKelamin(jk);
                        System.out.println("Input pekerjaan :");
                        pk = scn.nextLine();
                        st.setPekerjaan(pk);
                        System.out.println("Input golongan darah :");
                        gd = scn.nextLine();
                        st.setGolonganDarah(gd);
                        System.out.println("Input tempat lahir :");
                        tl = scn.nextLine();
                        st.setTempatLahir(tl);
                        System.out.println("Input tanggal lahir :");
                        tgl = scn.nextLine();
                        st.setTanggalLahir(tgl);
                        System.out.println("Input kewarganegaraan :");
                        k = scn.nextLine();
                        st.setKewarganegaraan(k);
                        dataList.add("" + st.getNama() + " " + st.getAgama() + " " + st.getJenisKelamin() + " " + st.getPekerjaan() + " " + st.getGolonganDarah() + " " + st.getKelurahan() + " " + st.getRT() + " " + st.getTempatLahir() + " " + st.getTanggalLahir() + " " + st.getKewarganegaraan() + ""); 
                        dataHash.put(st.getNama(), dataList);
                        break;
                    case 4:
                        st.setRT("04");
                        System.out.println("Input nama :");
                        n = scn.nextLine();
                        st.setNama(n);
                        System.out.println("Input agama :");
                        a = scn.nextLine();
                        st.setAgama(a);
                        System.out.println("Input jenis kelamin :");
                        jk = scn.nextLine();
                        st.setJenisKelamin(jk);
                        System.out.println("Input pekerjaan :");
                        pk = scn.nextLine();
                        st.setPekerjaan(pk);
                        System.out.println("Input golongan darah :");
                        gd = scn.nextLine();
                        st.setGolonganDarah(gd);
                        System.out.println("Input tempat lahir :");
                        tl = scn.nextLine();
                        st.setTempatLahir(tl);
                        System.out.println("Input tanggal lahir :");
                        tgl = scn.nextLine();
                        st.setTanggalLahir(tgl);
                        System.out.println("Input kewarganegaraan :");
                        k = scn.nextLine();
                        st.setKewarganegaraan(k);
                        dataList.add("" + st.getNama() + " " + st.getAgama() + " " + st.getJenisKelamin() + " " + st.getPekerjaan() + " " + st.getGolonganDarah() + " " + st.getKelurahan() + " " + st.getRT() + " " + st.getTempatLahir() + " " + st.getTanggalLahir() + " " + st.getKewarganegaraan() + ""); 
                        dataHash.put(st.getNama(), dataList);
                        break;
                }
                    case 5:
                        st.setRT("05");
                        System.out.println("Input nama :");
                        n = scn.nextLine();
                        st.setNama(n);
                        System.out.println("Input agama :");
